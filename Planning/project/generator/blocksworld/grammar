?start: pddl_problem

pddl: "(" definition domain objects init goal ")"

definition: "define" "(" "problem" string ")"

domain: "(" ":domain" string ")"

objects: "(" ":objects" string+ ")"

init: "(" ":init" predictate+ ")"

goal:

step: number ":" predicate

predicate: action_name arg+

name: string
arg: string

time: "time" "spent:" number "seconds" "instantiating" number "easy," number "hard" "action" "templates"

string :  /\w+(-\w+)*/

number: /\d+(\.?\d+)?/


%import common.WS
%ignore WS